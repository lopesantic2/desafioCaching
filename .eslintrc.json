{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "object-curly-spacing": 0,
        "new-cap": 0,
        "space-before-function-paren": 0,
        "linebreak-style": [ "error", "windows" ],
        "indent": [ "error", 4]
    }
}
